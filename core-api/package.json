{
  "name": "core.api",
  "version": "1.0.0",
  "description": "Api principal de lan√ßamentos.",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "",
    "prebuild": "",
    "build": "nest build",
    "test": "",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "commit": "git-cz",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migration -d src/migrations",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n migration -d src/migrations",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^7.1.1",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.1.1",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.1.1",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.4",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.16.1",
    "nanoid": "^3.1.12",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "tmp": "^0.2.1",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.0",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.1.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rxjs": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/migrations/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
